Cloud.Events.show({
    event_id: '55130f8eac4547febdac2a75'
}, function (e) {
    if (e.success) {
        var event = e.events[0];
        alert('Success:\n' +
            'id: ' + event.id + '\n' +
            'name: ' + event.name + '\n' +
            'updated_at: ' + event.updated_at);
    } else {
        alert('Error:\n' +
            ((e.error && e.message) || JSON.stringify(e)));
    }
});