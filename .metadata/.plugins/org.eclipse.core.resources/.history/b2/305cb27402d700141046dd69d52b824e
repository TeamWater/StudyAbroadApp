
function doClick(e) {
    $.menu.open();
   } 

var Agenda = Titanium.UI.createButton({});
Agenda.addEventListener('click',function OpenAgenda()
{
   var OpenAgenda = Alloy.createController("Agenda").getView();
	Agenda.open();
});



var Map = Titanium.UI.createButton({});

Map.addEventListener('click',function OpenMap()
{
   var OpenMap = Alloy.createController("Map").getView();
	Map.open();
});



var Settings = Titanium.UI.createButton({});

Settings.addEventListener('click',function OpenSettings()
{
   var OpenSettings = Alloy.createController("Settings").getView();
	Settings.open();
});


$.index.open();