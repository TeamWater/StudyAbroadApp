
function doClick(e) {
    $.menu.open();
   } 
   
var AgendaButton = Ti.UI.createButton({
	title : 'Agenda',
	height : 200,
	width : 100,
	top : 10,
	left :10
});

AgendaButton.addEventListener('click', function() {
	var OpenAgenda = Alloy.createController("Agenda").getView();
	$.Agenda.open();
});





var MapButton = Ti.UI.createButton({
	title : 'Map',
	height : 200,
	width : 100,
	top : 10,
	left :10
});

MapButton.addEventListener('click', function() {
	$.Map.open();
});





var SettingsButton = Ti.UI.createButton({
	title : 'Settings',
	height : 200,
	width : 100,
	top : 10,
	left :10
});

SettingsButton.addEventListener('click', function() {
	var OpenAgenda = Alloy.createController("Settings").getView();
	$.Settings.open();
});
 
$.index.open();
