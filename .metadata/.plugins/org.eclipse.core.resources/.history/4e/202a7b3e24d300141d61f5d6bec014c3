function doClick(e) {
    $.menu.open();
   } 


Cloud.Events.show({
    event_id: savedEventId
}, function (e) {
    if (e.success) {
        var event = e.events[0];
        alert('Success:\n' +
            'id: ' + event.id + '\n' +
            'name: ' + event.name + '\n' +
            'updated_at: ' + event.updated_at);
    } else {
        alert('Error:\n' +
            ((e.error && e.message) || JSON.stringify(e)));
    }
});